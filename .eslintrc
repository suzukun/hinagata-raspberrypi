{
  "root": true,
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": 1,
    "no-undef": 1,
    "no-shadow": 1,
    "no-param-reassign" : 1
  }
}
